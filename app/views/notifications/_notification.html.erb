<%= render partial: 'like_notification' %>
<%= render partial: 'clip_notification' %>
<%= render partial: 'follow_notification' %>
<%= render partial: 'comment_notification' %>




=begin

%>

<% if notification.notified_type == "like" %>
  <!-- 通知対象の投稿が投稿されてから30分以内-->
  <% if passed_time(notification) =< 1800 %>
    <%= render partial: 'like_one', locals: { notification: notification } %>
  <% else %>
    <!-- 通知対象の投稿が投稿されてから1時間後-->
    <% if passed_time(notification)/(24*60*60) >= 1 %>
      <!-- 対象の通知のうち最新の投稿に対してのみレンダリング-->
      <% if like_some_notification?(notification) == "true" %>
        <%= render partial: 'like_some', locals: { notification: notification } %>
      <% end %>
    <% end %>
  <% end %>

<% elsif notification.notified_type == "clip" %>
  <!-- 通知対象の投稿が投稿されてから30分以内 -->
  <% if passed_time(notification) =< 1800 %>
    <%= render partial: 'clip_one', locals: { notification: notification } %>
  <% else %>
    <!-- 通知対象の投稿が投稿されてから1時間後 -->
    <% if passed_time(notification)/(24*60*60) >= 1 %>
      <!-- 対象の通知のうち最新の投稿に対してのみレンダリング -->
      <% if clip_some_notification? == "true" %>
        <%= render partial: 'clip_some', locals: { notification: notification } %>
      <% end %>
    <% end %>
  <% end %>

<% elsif notification.notified_type == "follow" %>
  <%= render partial: 'follow_notification', locals: { notification: notification } %>

<% elsif notification.notified_type == "comment" %>
  <%= render partial: 'comment_notification', locals: { notification: notification } %>
<% else %>

<% end %>

